    "NAME": "hb_idleAdd()",
    "CATEGORY": "API",
    "SUBCATEGORY": "Idle states",
    "ONELINER": "Adds the background task.",
    "SYNTAX": "hb_idleAdd( <bAction> ) --> nHandle",
