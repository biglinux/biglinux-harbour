  {
    "_COMPONENT": "harbour",
    "_LANG": "en",
    "_DOCSOURCE": "./doc/en/datetime.txt",
    "AUTHOR": "Copyright 2000 Luiz Rafael Culik <culik@sl.conex.net>",
    "TEMPLATE": "Function",
    "NAME": "CMonth()",
    "CATEGORY": "API",
    "SUBCATEGORY": "Date/Time",
    "ONELINER": "Return the name of the month.",
    "SYNTAX": "CMonth( <dDate> ) --> cMonth",
